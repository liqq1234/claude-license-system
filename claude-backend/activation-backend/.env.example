# 环境配置示例文件
# 复制此文件为 .env 并修改相应的配置

# 应用环境
NODE_ENV=development

# 服务器配置
PORT=8888

# MySQL 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=license_server
DB_USER=root
DB_PASSWORD=123456

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=123456
REDIS_DB=0

# RSA 密钥配置
RSA_PRIVATE_KEY=sample.private.pem
RSA_PUBLIC_KEY=sample.public.pem
RSA_PASSPHRASE=1234

# 激活码配置
DEFAULT_EXPIRE_AFTER=31536000000
MAX_DEVICES_PER_CODE=5

# 缓存配置
CACHE_ACTIVATION_CODE_EXPIRY=3600
CACHE_DEVICE_BINDING_EXPIRY=1800
CACHE_STATS_EXPIRY=300

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log

# 虎皮椒支付配置
HUPIJIAO_APPID=你的虎皮椒APPID
HUPIJIAO_APPSECRET=你的虎皮椒AppSecret

# 域名配置
DOMAIN=https://你的域名.com

# Midjourney 配置
MIDJOURNEY_BASE_URL=http://localhost:6015
